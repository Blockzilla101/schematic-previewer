#!/usr/bin/env sh

./gradlew dist --no-daemon -q

cp  ./build/libs/previewer.jar ../Botzilla/schematic-previewer.jar

scp ./build/libs/previewer.jar nydus6:/root/schematic-previewer.jar
ssh nydus6 cp /root/schematic-previewer.jar /root/Botzilla/schematic-previewer.jar
ssh nydus6 rm /root/schematic-previewer.jar
